<?xml version="1.0" encoding="UTF-8"?>
<fop version="1.0">
  <strict-configuration>true</strict-configuration>
  <renderers>
    <renderer mime="application/pdf">
      <!-- Turn on tagged/accessible PDF -->
      <accessibility>true</accessibility>
      <!-- Optional but recommended: better structure tree order -->
      <pdf-a-mode>disabled</pdf-a-mode>
    </renderer>
  </renderers>
</fop>
